{
  "name": "nuxt-module-eslint-config",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.14.1",
  "description": "Generate ESLint config from current Nuxt settings",
  "license": "MIT",
  "repository": "antfu/nuxt-module-eslint-config",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepare": "nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "lint:play": "cd playground && eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "eslint": "^8.55.0"
  },
  "dependencies": {
    "@eslint/js": "^8.56.0",
    "@nuxt/devtools-kit": "^1.0.8",
    "@nuxt/kit": "^3.9.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-flat-config-viewer": "^0.1.11",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-unimport": "^0.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "get-port-please": "^3.1.2",
    "pathe": "^1.1.2",
    "unimport": "^3.7.1",
    "vue-eslint-parser": "^9.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.1",
    "@nuxt/test-utils": "^3.9.0",
    "@types/node": "^20.11.1",
    "@typescript-eslint/scope-manager": "^6.18.1",
    "bumpp": "^9.2.1",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-factory": "^0.0.2",
    "eslint-ts-patch": "8.56.0-0",
    "nuxt": "^3.9.1",
    "nuxt-module-eslint-config": "workspace:*",
    "vitest": "^1.2.0"
  }
}

{
  "name": "nuxt-module-eslint-config",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.11.0",
  "description": "Generate ESLint config from current Nuxt settings",
  "license": "MIT",
  "repository": "antfu/nuxt-module-eslint-config",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepare": "nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-unimport": "^0.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "pathe": "^1.1.1",
    "unimport": "^3.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/node": "^20.10.4",
    "@typescript-eslint/scope-manager": "^6.13.2",
    "bumpp": "^9.2.0",
    "eslint": "npm:eslint-ts-patch@8.55.0-1",
    "eslint-factory": "^0.0.1",
    "eslint-ts-patch": "8.55.0-1",
    "nuxt": "^3.8.2",
    "vitest": "^1.0.2"
  }
}
